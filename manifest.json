{
    "name": "s1mple Adblocker",
    "version": "1.0",
    "description": "This is an Ad Blocker to avoid Ads on Chrome Browser",
    "permissions": ["activeTab","webRequest","webRequestBlocking","<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "logos/16.png",
        "48": "logos/48.png",
        "128": "logos/128.png"
    },
    "browser_action": {
        "default_icon": "logos/16.png",
        "default_popup": "adblocker.html"
    },
    "manifest_version": 2
}